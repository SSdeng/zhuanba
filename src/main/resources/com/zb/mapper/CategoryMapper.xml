<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zb.mapper.CategoryMapper">

    <resultMap id="CategoryResultMap" type="com.zb.pojo.Category">
        <id column="category_id" jdbcType="INTEGER" property="id"/>
        <result column="category_name" jdbcType="VARCHAR" property="name"/>
        <result column="category_deleted" jdbcType="INTEGER" property="deleted"/>
        <result column="category_createTime" jdbcType="TIMESTAMP" property="createTime"/>
        <result column="category_updateTime" jdbcType="TIMESTAMP" property="updateTime"/>
    </resultMap>

    <sql id="Category_Column_List">
        category.id as category_id, category.name as category_name, category.deleted as category_deleted,
            category.create_time as category_createTime, category.update_time as category_updateTime
    </sql>


</mapper>